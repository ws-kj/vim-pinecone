\\

fizzBuzz: 1, 20

fizzBuzz :: {start: Int, end: Int}: (

    i: in.start | i <= in.end | i: i+1 @ (
        
        i % 3 = 0 && i % 5 = 0 ?
            print: "FizzBuzz"
        |
        i % 3 = 0 ?
            print: "Fizz"
        |
        i % 5 = 0 ?
            print: "Buzz"
        |
        print: i
    )
)        
